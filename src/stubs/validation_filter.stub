<?php

namespace DummyNamespace;

use RGilyov\CsvImporter\BaseValidationFilter;

class DummyClass extends BaseValidationFilter
{
    /**
     * @return string
     */
    protected $name = 'bad_word_validation';

    /**
     * @param array $csvItem
     * @return bool
     */
    public function filter(array $csvItem)
    {
        if (strpos($csvItem['name'], 'some bad word') !== false) {
            return false;
        }

        return true;
    }
}
